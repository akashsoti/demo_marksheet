<!DOCTYPE html>
<html>
	<head>
  	<title>Marksheet</title>
 		<%= stylesheet_link_tag    "application", :media => "all" %>
  	
  	<%= csrf_meta_tags %>
	</head>
	<body>
		<div class = "container">	
			<% flash.each do |key, value| %>
  			<div class="alert alert-<%= key %>"><%= value %></div>
			<% end %>
			<%= yield %>
			<canvas id="stripes">
		</div>
		<%= javascript_include_tag "application" %>
	</body>
</html>
